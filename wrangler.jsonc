{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "zlibrary-cloudflare",
  "main": "src/index.ts",
  "compatibility_date": "2025-05-02",
  "preview_urls": false,  //关闭预览url
  "routes": [{"custom_domain": true,"pattern": "redirect.sereniblue.com"},
  {"custom_domain": true,"pattern": "zb.sereniblue.com"}], //自定义域名,
  "kv_namespaces": [
    {
      "binding": "kv",//不要更换kv绑定名称
      "id": "299aeb7678e94d03a2a2a21ecf6c3769"
    }
  ],
  "env": {
    "production": {
      "name": "zlibrary-cloudflare-prod",
      "routes": [
        {"custom_domain": true, "pattern": "redirect.sereniblue.com"},
        {"custom_domain": true, "pattern": "zb.sereniblue.com"}
      ],
      "kv_namespaces": [
        {
          "binding": "kv",
          "id": "299aeb7678e94d03a2a2a21ecf6c3769"
        }
      ]
    },
    "staging": {
      "name": "zlibrary-cloudflare-staging",
      "routes": [
        {"custom_domain": true, "pattern": "staging.redirect.sereniblue.com"}
      ],
      "kv_namespaces": [
        {
          "binding": "kv",
          "id": "staging-kv-namespace-id"
        }
      ]
    },
    "development": {
      "name": "zlibrary-cloudflare-dev",
      "kv_namespaces": [
        {
          "binding": "kv",
          "id": "dev-kv-namespace-id"
        }
      ]
    }
  }
}
